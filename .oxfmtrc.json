{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 100,
  "experimentalSortImports": {},
  "experimentalSortPackageJson": true,
  "ignorePatterns": [
    "dist",
    ".nuxt",
    ".output",
    "node_modules",
    "coverage",
    "*.min.js",
    "pnpm-lock.yaml"
  ]
}
